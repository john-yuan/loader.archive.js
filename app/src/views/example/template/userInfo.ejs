<table>
    <tbody>
        <tr>
            <td>Current View Name</td>
            <td><%=
                // 调用传入进来的函数
                getViewName();
            %></td>
        </tr>
        <tr>
            <td>Project Name</td>
            <td><%= projectName %></td>
        </tr>
        <tr>
            <td>Project GitHub</td>
            <td><a href="<%= projectGithub %>" target="_blank"><%= projectGithub %></a></td>
        </tr>
        <tr>
            <td>Templating Language</td>
            <td><a href="http://ejs.co/" target="_blank">ejs</a></td>
        </tr>
        <%
            var funcDemo = function () {
                return 'This line is generated by a function in the ejs template.';
            };
        %>
        <tr>
            <td>Function Demo</td>
            <td><%=
                // 执行 function 并输出其返回结果
                funcDemo()
            %></td>
        </tr>
    </tbody>
</table>